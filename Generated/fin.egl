<html>
	<head>
		<meta charset="UTF-8"> 
		<title>Recapitulatif</title>
		<link href="../lib/css/bootstrap.min.css" rel="stylesheet">
	</head>

	<body>
		<div class="container text-center">
			<h2>Recapitulatif</h2>
			<ul id="recap" class="list-group">
				
			</ul>
		</div>
	
		<script src="../lib/js/jquery.min.js"></script>
		<script type="text/javascript">
			JSON.parse(localStorage.getItem("recap")).forEach(function(item){
				str = '<li class="list-group-item">'+item.intitule + ' ' + item.reponse + '</li>';
				$('#recap').append(str);
			});	
		</script>
	</body>
</html>